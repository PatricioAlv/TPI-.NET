# Configuraci√≥n de ngrok para exponer los servicios locales
# Copia este archivo a ngrok.yml y reemplaza TU_TOKEN_AQUI con tu authtoken

version: "2"
authtoken: TU_TOKEN_AQUI

tunnels:
  auth:
    proto: http
    addr: 5001
  
  messages:
    proto: http
    addr: 5002
  
  groups:
    proto: http
    addr: 5003
